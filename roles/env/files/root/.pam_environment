# OK: ${HOME}
# OK: @{PAM_USER}
# not OK: @{HOME}
XDG_CACHE_HOME	DEFAULT=/root/.cache		OVERRIDE=${XDG_CACHE_HOME}
XDG_CONFIG_HOME	DEFAULT=/root/.config		OVERRIDE=${XDG_CONFIG_HOME}
XDG_DATA_HOME	DEFAULT=/root/.local/share	OVERRIDE=${XDG_DATA_HOME}
XDG_STATE_HOME	DEFAULT=/root/.xdg/state	OVERRIDE=${XDG_STATE_HOME}
